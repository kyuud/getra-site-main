{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "barryvdh/laravel-dompdf": "^2.0.1",
        "biscolab/laravel-recaptcha": "^6.0.0",
        "bmatovu/laravel-xml": "^3.0",
        "cocur/slugify": "^4.0",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.38.2",
        "laravel/sanctum": "^3.3",
        "laravel/tinker": "^2.0",
        "livewire/livewire": "^3.3",
        "maatwebsite/excel": "^3.1",
        "mtownsend/response-xml": "^2.2.0",
        "pgrimaud/instagram-user-feed": "^6.2",
        "spatie/array-to-xml": "^3.2.2",
        "spatie/html-element": "^1.1.4",
        "spatie/laravel-html": "^3.3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "laravel/ui": "^4.3.0",
        "nunomaduro/collision": "^7.10.0",
        "phpunit/phpunit": "^10.5.3",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "files": [
            "app/Helpers/functions.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
